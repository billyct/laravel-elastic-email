{
    "name": "billyct/laravel-elastic-email",
    "authors": [
        {
            "name": "billyct",
            "email": "billy_allen@126.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "elastic-email/web-api-client": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Billyct\\ElasticEmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Billyct\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Billyct\\ElasticEmail\\ElasticEmailServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.5"
    }
}
